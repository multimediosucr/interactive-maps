{% assign posts = site.posts | where:"type", "youtube" %}
/* Number of places: {{ posts | size }} */
processJSON([
  {% for post in posts %}
    {
      "guid": "{{ post.guid }}",
      "title": "{{ post.title }}",
      "latlng": "{{ post.latlng }}",
      "videoId": "{{ post.videoId }}"
    }
    {% unless forloop.last %},{% endunless %}
  {% endfor %}
]);